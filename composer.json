{
    "name": "contaoblackforest/contao-calendar-filter-tag",
    "description": "This extend the contao-calendar-filter module add tag for filter for Contao CMS",
    "keywords": [
        "contao",
        "module",
        "calendar",
        "tag",
        "filter"
    ],
    "type": "contao-module",
    "require": {
        "php": ">=5.3",
        "contao/core": ">=3.2,<4",
        "contao-community-alliance/composer-plugin": "~2.0",
        "contaoblackforest/contao-calendar-filter-tag": "dev-develop"
    },
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Sven Baumann",
            "email": "baumann.sv@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Dominik Tomasi",
            "email": "dominik.tomasi@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/ContaoBlackForest/contao-calendar-filter-tag/issues",
        "source": "https://github.com/ContaoBlackForest/contao-calendar-filter-tag"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "squizlabs/php_codesniffer": "~2.3",
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-4": {
            "ContaoBlackForest\\Module\\CalendarFilter\\Tag\\": "src/"
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "contao": "system/modules/calendar-filter-tag"
            }
        },
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
}
